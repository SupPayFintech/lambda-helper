{
  "name": "@suppay/lambda-helper",
  "version": "0.2.1",
  "description": "Suppay lambda lambda helper",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SupPayFintech/lambda-helper.git"
  },
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SupPayFintech/lambda-helper/issues"
  },
  "homepage": "https://github.com/SupPayFintech/lambda-helper#readme",
  "devDependencies": {
    "@aws-sdk/types": "^3.357.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/busboy": "^1.5.0",
    "@types/jest": "^29.5.2",
    "@types/sinon": "^10.0.15",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-plugin-jest": "^27.2.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "sinon": "^15.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@sentry/serverless": "^7.60.0",
    "axios": "^1.4.0",
    "busboy": "^1.6.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "radash": "^10.9.0",
    "zod": "^3.21.4"
  }
}
